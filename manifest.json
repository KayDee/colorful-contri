{
  "name": "GitExtension",
  "version": "1.0",
  "description": "Github Extension!",
  "manifest_version": 2,
  "permissions":["activeTab","declarativeContent"],
  "background": {
      "scripts": [
          "background.js"
      ],
      "persistent": false
  },

  "page_action": {
      "default_popup": "popup.html"
    }

}